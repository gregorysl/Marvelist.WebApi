<Window x:Class="JsonToSqlInsertParser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JsonToSqlInsertParser"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <StackPanel Margin="5" Orientation="Horizontal">
            <TextBlock VerticalAlignment="Center">Input file:</TextBlock>
            <TextBox Width="200"></TextBox>
            <Button>Browse</Button>
            <TextBlock VerticalAlignment="Center">Data type:</TextBlock>
            <ComboBox></ComboBox>
        </StackPanel>
    </StackPanel>
</Window>